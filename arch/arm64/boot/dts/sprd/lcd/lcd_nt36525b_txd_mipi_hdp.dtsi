						39 00 00 03 51 00 00
						15 00 00 02 53 2C
						15 00 00 02 55 00
						05 64 00 01 11
						05 14 00 01 29
						15 14 00 02 35 00
					];

					sprd,sleep-in-command =  [
						05 14 00 01 28
						05 64 00 01 10
					];

					sprd,sleep-out-command = [
						05 64 00 01 11
						05 14 00 01 29
					];

					sprd,cabc-ui-mode = [
						15 00 00 02 55 01
						];

					sprd,cabc-mov-mode = [
						15 00 00 02 55 02
						];

					sprd,cabc-off-mode = [
						15 00 00 02 55 00
						];
					oled-backlight {
						default-brightness-level = <1023>;
						sprd,max-level = <4095>;

						brightness-levels = [
							39 00 00 03 51 0F FF
						];
					};
					display-timings {
						native-mode = <&nt36525b_timing0>;

						nt36525b_timing0: timing0 {
							clock-frequency = <96000000>;
							hactive = <720>;
							vactive = <1600>;
							hback-porch = <60>;
							hfront-porch = <67>;
							vback-porch = <252>;
							vfront-porch = <12>;
							hsync-len = <7>;
							vsync-len = <2>;
						};
					};
				};
			};
		};
	};
};
